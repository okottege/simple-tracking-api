{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SimpleTaxDB": "Data Source=[Data_Source];Initial Catalog=[DB_Name];User Id=[User_Id];Password=[Password];",
    "CloudStorage": "[storage_conn_string]"
  },
  "ADSettings": {
    "ADInstance": "https://login.microsoftonline.com/{0}",
    "TenantId": "c501b7e7-96c1-4bb8-9380-785a1167f940",
    "AppIdUri": "https://simpleoshad.onmicrosoft.com/f1d46fd1-85d9-477f-b513-911ca14b1a0b"
  },
  "AuthenticationConfig": {
    "Authority": "https://osh-auth0-test.au.auth0.com/",
    "Audience": "https://test-tracking-service-api.azurewebsites.net",
    "ClientID": "[Client_ID]",
    "ClientSecret": "[Client_Secret]",
    "Realm": "Username-Password-Authentication",
    "GrantType": "http://auth0.com/oauth/grant-type/password-realm"
  },
  "UserManagementConfig": {
    "ClientID": "[Client_ID]",
    "ClientSecret": "[Client_Secret]",
    "GrantType": "client_credentials",
    "BaseUrl": "https://osh-auth0-test.au.auth0.com/api/v2/",
    "ConnectionID": "[Connection_ID]",
    "Audience": "https://osh-auth0-test.au.auth0.com/api/v2/"
  },
  "Storage": {
    "ContainerName": "[container_name]"
  }
}
